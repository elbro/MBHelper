<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://oddsfinder.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>waws-prod-am2-001.publish.azurewebsites.windows.net:443</MSDeployServiceURL>
    <DeployIisAppPath>OddsFinder</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <UserName>$OddsFinder</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="OddsContext" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:***REMOVED***.database.windows.net,1433;Initial Catalog=[oddsdb];User ID=***REMOVED***@***REMOVED***;Password=***REMOVED***" Name="Data Source=tcp:***REMOVED***.database.windows.net,1433;Initial Catalog=[oddsdb];User Id=***REMOVED***@***REMOVED***;Password=***REMOVED***;MultipleActiveResultSets=true" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="MBHelper.Models.OddsContext, MBHelper" MigrationConfiguration="MBHelper.Migrations.Configuration, MBHelper" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)OddsContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:***REMOVED***.database.windows.net,1433;Initial Catalog=[oddsdb];User Id=***REMOVED***@***REMOVED***;Password=***REMOVED***;MultipleActiveResultSets=true</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>